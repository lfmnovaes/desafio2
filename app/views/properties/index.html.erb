<p style="color: green"><%= notice %></p>

<h1 class="text-3xl font-bold underline">
  Properties
</h1>

<div id="properties">
  <% @properties.each do |property| %>
    <%= render property %>
    <p>
      <%= link_to "Show this property", property %>
    </p>
  <% end %>
</div>

<%= link_to "New property", new_property_path %>
